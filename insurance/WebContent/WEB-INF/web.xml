<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>insurance</display-name>
  <servlet>
    <servlet-name>InsuranceListServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.InsuranceListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsuranceListServlet</servlet-name>
    <url-pattern>/type/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InsuranceDetailServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.InsuranceDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsuranceDetailServlet</servlet-name>
    <url-pattern>/detail/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetCodeServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.GetCodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetCodeServlet</servlet-name>
    <url-pattern>/code</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OrderQueryServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.OrderQueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderQueryServlet</servlet-name>
    <url-pattern>/order/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OrderSubmitServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.OrderSubmitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderSubmitServlet</servlet-name>
    <url-pattern>/ordersub</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OrderDetailServlet</servlet-name>
    <servlet-class>com.infy.insurancemall.action.OrderDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderDetailServlet</servlet-name>
    <url-pattern>/orderdetail/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AxisServlet</servlet-name>
    <servlet-class>org.apache.axis2.transport.http.AxisServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>/servlet/AxisServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>*.jws</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
</web-app>